
<%= form_with(model: @review ) do |f|%>
    <div>
        <%#byebug %>
        <%=  f.label :rating%>
        <br>
        <%= f.text_field :rate, class:"form-text" , autofocus: true  %>
    </div>
    <div>
        <%= f.label :body%>
        <br>
        <%= f.text_area :body, cols: 50, rows: 5 %>
    </div>
    <%= f.submit "Review"%>
<% end %>